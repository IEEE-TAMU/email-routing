{
    "compilerOptions": {
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "moduleResolution": "bundler",
      "target": "ESNext",
      "rootDir": "src",
      "outDir": "lib/cjs",
      "module": "ESNext",
      "lib": ["ESNext", "DOM"],
      "types": ["@cloudflare/workers-types"],
    },
    "include": ["src/**/*"]
  }